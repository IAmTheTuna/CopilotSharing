      "isMarkdownCharacter": false,
      "matchPosition": 6,
      "regex": "(\\[)(.*?)(\\])(\\()(.*?)(\\))",
      "color": "#a7c080"
    },

    "sidebarIconColorOverride": "#3A515D",

    "line-contains-⟁": {
      "backgroundColor": "#e67e80",
      "matchPosition": 0,
      "regex": ".*⟁.*",
      "color": "#FFFFFF",
      "borderRadius": 6,
      "leftBorderPadding": 6,
      "rightBorderPadding": 6
    },

    "line-contains-⚐": {
      "backgroundColor": "#171C1E",
      "matchPosition": 0,
      "regex": ".*⚐.*",
      "color": "#DCD158",
      "borderRadius": 6,
      "leftBorderPadding": 6,
      "rightBorderPadding": 6
    },

    "line-contains-☆": {
      "backgroundColor": "#171C1E",
      "matchPosition": 0,
      "regex": ".*☆.*",
      "color": "#F4A522",
      "borderRadius": 6,
      "leftBorderPadding": 6,
      "rightBorderPadding": 6
    },

    "line-contains-⎋": {
      "backgroundColor": "#425047",
      "matchPosition": 0,
      "regex": ".*⎋.*",
      "color": "#C7C0AE",
      "borderRadius": 6,
      "leftBorderPadding": 6,
      "rightBorderPadding": 6
    },

    "line-contains-¿": {
      "backgroundColor": "#9da9a0",
      "matchPosition": 0,
      "regex": ".*¿.*",
      "color": "#000000"
    },

    "hidden-comment": {
      "regex": "(%%)([^%]{1,})(%%)",
      "matchPosition": 2,
      "isRevealOnCursorRange": true,
      "isHiddenWithoutCursor": true,
      "color": "#888888"
    },

    "hidden-comment-left-colon": {
      "regex": "(%%)([^:]{1,})(%%)",
      "matchPosition": 1,
      "isRevealOnCursorRange": true,
      "isHiddenWithoutCursor": false,
      "isMarkdownCharacter": true,
      "color": "#88818475"
    },

    "hidden-comment-right-colon": {
      "regex": "(%%)([^:]{1,})(%%)",
      "matchPosition": 3,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true,
      "color": "#88818475"
    },

    "line-comment-hide": {
      "regex": "(.*)( // )(.*)",
      "matchPosition": 3,
      "isHiddenWithoutCursor": true,
      "color": "#AA888888"
    },

    "line-comment-show": {
      "regex": "(.* // .*)",
      "matchPosition": 1,
      "isRevealOnCursorRange": true
    },

    "line-comment-slash": {
      "regex": "(.*)( // )",
      "matchPosition": 2,
      "isMarkdownCharacter": true,
      "color": "#888888"
    }
  }
}
