    },

    "code-right-backtick": {
      "color": "#66B9B279"
    },

    "checked": {
      "color": "#a7c080",
      "headIndent": 33
    },

    "checked-canceled": {
      "strikethroughStyle": 1,
      "matchPosition": 0,
      "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] )(.*)",
      "color": "#928383"
    },

    "checked-scheduled": {
      "regex": "(^\\h*[\\*\\-]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\>\\] )(.*)",
      "matchPosition": 0,
      "color": "#7FBBB3"
    },

