      "fence-open": {
        "color": "#6584A2"
      },
      "fence-close": {
        "color": "#6584A2"
      },
      "language-specifier": {
        "font": "JetBrainsMono-Italic",
        "color": "#afafaf",
        "size": 14,
        "paragraphSpacing": 0
      }
    },

