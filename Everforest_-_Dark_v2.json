      "font": "AppleSystemUIFontBold"
    },

    "title-mark1": {
      "color": "#9B8D70",
      "size": 28,
      "font": "AppleSystemUIFontBold",
      "paragraphSpacingBefore": 2
    },

    "title1-highlighted-full-width": {
      "regex": "(^#[[:blank:]])(.*?)(==)",
      "matchPosition": 2,
      "isRevealOnCursorRange": true,
      "backgroundColor": "#C7C0AE",
      "color": "#232a2e",
      "borderRadius": 6,
      "inlineBorder": true,
      "leftBorderPadding": 4,
      "rightBorderPadding": 4,
      "isFullWidthBorder": true
    },

    "title1-highlighted-full-width-left-marker": {
      "regex": "(^#[[:blank:]])(.*?)(==)",
      "matchPosition": 1,
      "isMarkdownCharacter": true,
      "isHiddenWithoutCursor": true,
      "isRevealOnCursorRange": true,
      "color": "#7A8B7E",
      "backgroundColor": "#C7C0AE"
    },

    "title1-highlighted-full-width-right-marker": {
      "regex": "(^#[[:blank:]])(.*?)(==)",
