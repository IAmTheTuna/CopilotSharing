    "NoteLinks-left-brackets": {
      "isRevealOnCursorRange": true,
      "isHiddenWithoutCursor": false,
      "isMarkdownCharacter": true,
      "matchPosition": 1,
      "regex": "([[:blank:]]?\\[\\[)(.*?)(\\]\\])",
      "color": "#a7c080"
    },

    "NoteLinks-right-brackets": {
      "isRevealOnCursorRange": true,
      "isHiddenWithoutCursor": false,
      "isMarkdownCharacter": true,
      "matchPosition": 3,
      "regex": "([[:blank:]]?\\[\\[)(.*?)(\\]\\])",
      "color": "#a7c080"
    },

    "NoteLinks-main-cancelled": {
      "color": "#EB2E2E",
      "backgroundColor": "#000000"
    },

    "URL-links-left-bracket": {
      "isRevealOnCursorRange": true,
      "isHiddenWithoutCursor": false,
      "isMarkdownCharacter": false,
      "matchPosition": 1,
      "regex": "(\\[)(.*?)(\\])(\\()(.*?)(\\))",
      "color": "#FF6600"
    },

    "URL-links-right-bracket": {
      "isRevealOnCursorRange": true,
      "isHiddenWithoutCursor": false,
      "isMarkdownCharacter": false,
      "matchPosition": 3,
      "regex": "(\\[)(.*?)(\\])(\\()(.*?)(\\))",
      "color": "#FF6600"
    },

    "URL-links-far-left-bracket": {
      "isRevealOnCursorRange": true,
      "isHiddenWithoutCursor": false,
      "isMarkdownCharacter": false,
      "matchPosition": 4,
      "regex": "(\\[)(.*?)(\\])(\\()(.*?)(\\))",
